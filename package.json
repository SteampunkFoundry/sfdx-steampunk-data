{
  "name": "@steampunk/sfdx-data",
  "description": "Steampunk's SFDX Data Plugin",
  "version": "0.1.1",
  "author": "Steampunk",
  "bugs": "https://github.com/SteampunkFoundry/steampunk-sfdx-data/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@salesforce/command": "^5.1.0",
    "@salesforce/core": "^2.36.4",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "fs-extra": "^10.1.0",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/test": "^2.1.0",
    "@salesforce/dev-config": "^3.0.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.31",
    "chai": "^4.2.0",
    "globby": "^13.1.1",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^13.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/SteampunkFoundry/steampunk-sfdx-data",
  "keywords": [
    "sfdx-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "sfdx",
    "topics": {
      "punk:data": {
        "description": "Commands for working with data."
      }
    },
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "SteampunkFoundry/steampunk-sfdx-data",
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  }
}
