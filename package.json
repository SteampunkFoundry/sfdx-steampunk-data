{
  "name": "@steampunk/sfdx-steampunk-data",
  "description": "Steampunk's SFDX Data Plugin",
  "version": "0.2.0",
  "author": "Steampunk",
  "license": "MIT",
  "homepage": "https://github.com/SteampunkFoundry/sfdx-steampunk-data",
  "repository": "SteampunkFoundry/sfdx-steampunk-data",
  "bugs": "https://github.com/SteampunkFoundry/sfdx-steampunk-data/issues",
  "keywords": [
    "sfdx-plugin"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "files": [
    "/lib",
    "/messages",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "main": "lib/index.js",
  "oclif": {
    "bin": "sfdx",
    "commands": "./lib/commands",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "plugins": [
      "@oclif/plugin-version"
    ],
    "topics": {
      "punk:data": {
        "description": "Commands for working with data."
      }
    }
  },
  "dependencies": {
    "@oclif/core": "1.8.1",
    "@oclif/plugin-version": "^1.0.4",
    "@salesforce/core": "^3.18.1",
    "@salesforce/sf-plugins-core": "^1.11.3",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "fs-extra": "^10.0.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.10",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/test": "^2.1.0",
    "@salesforce/cli-plugins-testkit": "^1.5.35",
    "@salesforce/dev-config": "^3.0.1",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.22",
    "chai": "^4.3.6",
    "globby": "^11.1.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "lint": "tslint --project . --config tslint.json --format stylish",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "test-nut": "mocha **/*.nut.ts",
    "version": "oclif-dev readme && git add README.md"
  }
}
